
class Solution {
    boolean isPalindrome(String s) {
        // code here
        int n = s.length();
        boolean flag = false;
        for(int i=0;i<n;i++){
            if(s.charAt(i) == s.charAt(n-1-i)){
                flag = true;
            }
            else{
                flag = false;
                break;
            }
        }
        return flag;
    }
};